{
  "name": "my-playwright-project",
  "version": "1.0.0",
  "description": "End-to-end tests using Playwright with TypeScript",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev:server": "tsx src/server.ts",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "debug": "cross-env PWDEBUG=1 playwright test",
    "debug:file": "cross-env PWDEBUG=1 playwright test",
    "debug:grep": "cross-env PWDEBUG=1 playwright test -g",
    "debug:edge": "cross-env PWDEBUG=1 playwright test --project='Microsoft Edge'",
    "debug:chrome": "cross-env PWDEBUG=1 playwright test --project='Google Chrome'",
    "debug:last": "cross-env PWDEBUG=1 playwright test --last-failed",
    "report": "playwright show-report",
    "codegen:edge": "start-server-and-test \"npm:dev:server\" http://localhost:3000 \"playwright codegen --channel=msedge http://localhost:3000\"",
    "codegen:chrome": "start-server-and-test \"npm:dev:server\" http://localhost:3000 \"playwright codegen --channel=chrome http://localhost:3000\"",
    "test:edge": "playwright test --project='Microsoft Edge'",
    "test:chrome": "playwright test --project='Google Chrome'",
    "update-snapshots": "playwright test --update-snapshots",
    "trace:open": "playwright show-trace",
    "commit:patch": "git add . && git commit -m && npm version patch",
    "commit:minor": "git add . && git commit -m && npm version minor",
    "commit:major": "git add . && git commit -m && npm version major",
    "preversion": "npm test",
    "postversion": "git push --follow-tags origin main"
  },
  "keywords": [
    "playwright",
    "typescript",
    "e2e",
    "testing",
    "automation"
  ],
  "author": "Brittany Bales",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "cross-env": "^10.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
